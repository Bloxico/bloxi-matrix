{
  "name": "@filecoin/web",
  "version": "1.0.3",
  "private": false,
  "description": "FILECOIN web application",
  "author": "Aleksa Toljic <aleksa.toljic@bloxico.com>",
  "license": "ISC",
  "main": "src/main.tsx",
  "scripts": {
    "start": "http-server dist -P http://localhost:8080? -g",
    "preserve": "yarn prebuild",
    "serve": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "prebuild": "concurrently \"yarn generate:routes\" \"yarn generate:behaviors\"",
    "generate:routes": "node scripts/routes.js",
    "generate:behaviors": "yarn combine-behaviors",
    "build": "webpack --config ./config/webpack.prod.js",
    "convert-airtable": "ts-node --compiler-options='{\"module\":\"commonjs\"}' scripts/data/convertAirtable.ts",
    "convert-yaml": "yq -j eval scripts/data/behaviors.yml > src/behaviors.json",
    "generate-test-data": "ts-node --compiler-options='{\"module\":\"commonjs\"}' scripts/data/generateTestData.ts",
    "combine-behaviors": "ts-node --compiler-options='{\"module\":\"commonjs\"}' scripts/data/combineBehaviors.ts"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "devDependencies": {
    "@babel/runtime": "^7.14.6",
    "@types/react-router-dom": "^5.1.6",
    "concurrently": "^6.0.0",
    "csv-parse": "^5.0.3",
    "http-server": "^14.0.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^4.0.0",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "@filecoin/build": "^1.0.0",
    "@filecoin/core": "^1.0.0",
    "@filecoin/ui": "^1.0.0",
    "react-router-dom": "^6.0.2",
    "unstated-next": "^1.1.0",
    "yaml": "^1.10.2"
  }
}
